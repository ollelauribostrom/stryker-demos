{
  "name": "stryker-react-ts-example",
  "version": "1.0.0",
  "description": "An example",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "build-watch": "webpack --watch --mode development",
    "dev-server": "webpack-dev-server",
    "test": "webpack --mode production && npm run jest",
    "jest": "jest",
    "coverage": "jest --coverage --no-cache",
    "mutate": "./node_modules/.bin/stryker run && ./node_modules/.bin/opn tests/mutation/html/index.html",
    "clean": "rm -r dist/*",
    "tslint": "tslint -p ."
  },
  "jest": {
    "coverageDirectory": "<rootDir>/tests/coverage",
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/tests/**/**/**.spec.(ts|tsx|js)"
    ],
    "collectCoverageFrom": [
      "src/**/**.(ts|tsx|js)"
    ]
  },
  "author": "Olle Lauri Bostr√∂m",
  "license": "MIT",
  "dependencies": {
    "@types/flux": "^3.1.7",
    "@types/jszip": "^3.1.3",
    "@types/minimatch": "^3.0.3",
    "@types/mousetrap": "^1.6.0",
    "@types/pako": "^1.0.0",
    "@types/react-icons": "^2.2.5",
    "@types/react-modal": "^3.1.2",
    "@types/xterm": "^3.0.0",
    "cassowary": "0.0.2",
    "flux": "^3.1.3",
    "jszip": "^3.1.5",
    "minimatch": "^3.0.4",
    "monaco-editor": "^0.10.1",
    "mousetrap": "^1.6.1",
    "pako": "^1.0.6",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "react-modal": "^3.3.1",
    "tslint-react": "^3.5.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.1.4",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "awesome-typescript-loader": "^3.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.2",
    "opn-cli": "^3.1.0",
    "react-test-renderer": "^16.2.0",
    "source-map-loader": "^0.2.3",
    "stryker": "^0.22.4",
    "stryker-api": "^0.16.1",
    "stryker-babel-transpiler": "^0.4.2",
    "stryker-html-reporter": "^0.13.3",
    "stryker-javascript-mutator": "^0.6.3",
    "stryker-jest-runner": "^0.6.0",
    "stryker-typescript": "^0.10.2",
    "stryker-webpack-transpiler": "^0.3.4",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  }
}
